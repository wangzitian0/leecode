public class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] ans= new int[2];
        HashMap<int,int> mp = new HashMap<int,int>();
        int len = nums.length();
        for(int i=0;i<len;i++)
        {
            if(mp.get(target-nums[i])!=null)
            {
                ans[0]=mp.get(target-nums[i]);
                ans[1]=i+1;
                return ans;
            }
            mp.set(nums[i],i+1);
        }
    }
}
